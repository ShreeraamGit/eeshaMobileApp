{
  "title": "Basic",
  "argTypes": {
    "size": {
      "control": {
        "type": "select"
      },
      "options": ["sm", "md", "lg"],
      "defaultValue": "md"
    },
    "orientation": {
      "control": {
        "type": "select"
      },
      "options": ["horizontal", "vertical"],
      "defaultValue": "horizontal"
    },
    "isDisabled": {
      "control": {
        "type": "boolean"
      },
      "defaultValue": false
    },
    "isReversed": {
      "control": {
        "type": "boolean"
      },
      "defaultValue": false
    }
  },
  "reactLive": {
    "Slider": "@/components/ui/slider",
    "SliderTrack": "@/components/ui/slider",
    "SliderFilledTrack": "@/components/ui/slider",
    "SliderThumb": "@/components/ui/slider",
    "Center": "@/components/ui/center"
  }
}
