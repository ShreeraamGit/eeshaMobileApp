{
  "expo": {
    "name": "Eesha Silks",
    "slug": "eesha-silks",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eeshasilks.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take photos for product reviews.",
        "NSPhotoLibraryUsageDescription": "This app accesses the photo library to select images for product reviews."
      }
    },
    "android": {
      "package": "com.eeshasilks.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
    },
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "your-project-id-here"
      }
    },
    "owner": "eesha-silks"
  }
}
